{
    "name": "redicon/cms_articles",
    "license": "MIT",
    "description": "CMS ZarzÄ…dzanie podstronami",
    "homepage": "https://redicon.pl",
    "type": "library",
    "authors": [{
        "name": "Tomasz Cwik",
        "email": "tomek@redicon.pl"
    }],
    "require": {
        "php": "^7.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Redicon\\CMS_Articles\\": "src/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Redicon\\CMS_Articles\\": "src/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.7-dev"
        },
        "laravel": {
            "providers": [
                "Redicon\\CMS_Articles\\CmsServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-package-install": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan migrate:fresh --path=vendor\\redicon\\cms_articles\\src\\database\\migrations",
            "php artisan db:seed --class=Redicon\\CMS_Articles\\Database\\Seeds\\DatabaseSeeder",
            "composer dump-autoload"
        ],
        "post-package-update": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan migrate --path=vendor\\redicon\\cms_articles\\src\\database\\migrations",
            "composer dump-autoload"
        ]
    }
}